  "version": "1.4.1",
  "bin": {
    "number-cruncher": "index.js"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "start http://localhost:8080 && http-server ./docs -a localhost -p 8080",
